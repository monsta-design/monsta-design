{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "e0d0a139-0420-4dc1-8630-d4df0c682968"
  },
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "library": {
      "projectType": "library",
      "root": "src/components",
      "sourceRoot": "src/components",
      "prefix": "ns",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "src/components/tsconfig.lib.json",
            "project": "src/components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "src/components/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/components/src/test.ts",
            "tsConfig": "src/components/tsconfig.spec.json",
            "karmaConfig": "src/components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/components/tsconfig.lib.json",
              "src/components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "doc": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "site/doc",
      "sourceRoot": "site/doc",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.partial.js"
            },
            "outputPath": "dist/doc",
            "index": "site/doc/index.html",
            "main": "site/doc/main.ts",
            "polyfills": "site/doc/polyfills.ts",
            "tsConfig": "site/tsconfig.app.json",
            "aot": true,
            "assets": [
              "site/doc/favicon.ico",
              "site/doc/assets"
            ],
            "styles": [
              "site/doc/styles.scss"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "site/doc/environments/environment.ts",
                  "with": "site/doc/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "doc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "doc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "doc:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "site/doc/test.ts",
            "polyfills": "site/doc/polyfills.ts",
            "tsConfig": "site/doc/tsconfig.spec.json",
            "karmaConfig": "site/doc/karma.conf.js",
            "assets": [
              "site/doc/favicon.ico",
              "site/doc/assets"
            ],
            "styles": [
              "site/doc/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "site/doc/tsconfig.app.json",
              "site/doc/tsconfig.spec.json",
              "site/doc/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "site/doc/e2e/protractor.conf.js",
            "devServerTarget": "document:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "document:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "library"
}
